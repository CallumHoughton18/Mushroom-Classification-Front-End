{
    "compilerOptions": {
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "./build/",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "target": "es5"
    },
    "include": ["./src/**/**/*"]
}
