{
    "type": "module",
    "name": "mushroom-classification-front-end",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "moduleNameMapper": {
            "\\.(css|less|sass|scss)$": "<rootDir>/src/__mocks__/styleMock.ts",
            "\\.(png)$": "<rootDir>/src/__mocks__/imageMock.ts"
        },
        "transform": {
            "\\.(ts|tsx)$": "ts-jest"
        },
        "testRegex": "/__tests__/.*\\.(ts|tsx)$",
        "reporters": [
            "default",
            "jest-junit"
        ],
        "coverageReporters": [
            "text",
            "cobertura"
        ]
    },
    "jest-junit": {
        "outputDirectory": ".",
        "outputName": "testresults.xml",
        "uniqueOutputName": "false",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}",
        "ancestorSeparator": " â€º ",
        "usePathForSuiteName": "true"
    },
    "scripts": {
        "dev": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server --mode development --env.development --open --hot --progress --port 5000",
        "build": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production --env.production --progress",
        "lint": "eslint './src/**/*.{ts,tsx}'",
        "analyze": "source-map-explorer build/*.js",
        "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.11.1",
        "@testing-library/react": "^10.4.7",
        "@testing-library/user-event": "^12.0.15",
        "@types/jest": "^26.0.5",
        "@types/node": "^14.0.9",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/webpack": "^4.41.17",
        "@types/webpack-dev-server": "^3.11.0",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "css-loader": "^3.5.3",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.4",
        "file-loader": "^6.0.0",
        "fork-ts-checker-webpack-plugin": "^4.1.6",
        "html-webpack-plugin": "^4.3.0",
        "jest": "^26.1.0",
        "jest-junit": "^11.1.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.0.5",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "ts-jest": "^26.1.3",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "source-map-explorer": "^2.4.2"
    }
}
